{
  "name": "school-management-api",
  "version": "1.0.0",
  "description": "School management SQL-based API",
  "main": "./src/index.js",
  "scripts": {
    "dev": "nodemon src/index.js",
    "test:ep": "cd ./tests/utils/CLI && node prepare.js && npx mocha run.js -exit && node cleanup.js",
    "test:cleanup": "cd ./tests/utils/CLI & node cleanup.js",
    "genprisma": "cd ./src/api/v1 && npx prisma generate",
    "seed": "cd ./src/api/v1 && prisma migrate reset --skip-generate && node ./prisma/seed.manual.js",
    "studio": "cd ./src/api/v1 && npx prisma studio"
  },
  "author": "Michał Podsiadły <podsiadly.michal@protonmail.com>",
  "license": "MIT",
  "dependencies": {
    "@prisma/client": "^3.0.1",
    "bcrypt": "^5.0.1",
    "body-parser": "^1.19.0",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "express-async-handler": "^1.1.4",
    "helmet": "^4.6.0",
    "joi": "^17.4.0",
    "jsonwebtoken": "^8.5.1",
    "morgan": "^1.10.0",
    "mysql2": "^2.2.5"
  },
  "devDependencies": {
    "chai": "^4.3.4",
    "chai-http": "^4.3.0",
    "chalk": "^4.1.1",
    "dotenv": "^10.0.0",
    "faker": "^5.5.3",
    "mocha": "^9.0.0",
    "nodemon": "^2.0.7",
    "prisma": "^2.25.0"
  }
}
